    "i-graphql": "^0.1.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "node-fetch": "^3.2.10",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@aexol/axolotl": "^0.5.1",
    "@types/jsonwebtoken": "^9.0.6",
